{
  "directories": {
    "$HOME/resources": ["resources", "rsc", "dbg"],
    "$HOME/resources/config/dotfiles": ["dotfiles"],
    "$HOME/resources/config/dotfiles/.config": ["resources-config", "rsc-config", "rsc-cfg"],
    "$HOME/resources/notes/commands": ["resources-commands", "commands", "cmds"],
    "$HOME/resources/notes/languages": ["resources-languages", "languages", "lang"],
    "$HOME/bin": ["bin"],
    "$HOME/Music/Sorted": ["music-sorted", "sorted", "Sorted"],
    "$HOME/Music/Unsorted": ["music-unsorted", "unsorted", "Unsorted"],
    "$HOME/Music/Sorted/Rated": ["music-rated", "rated", "Rated"],
    "$HOME/Music/Sorted/Unrated": ["music-rated", "unrated", "Unrated"],
    "$HOME/.config/i3": ["i3"],
    "$HOME/.config/polybar": ["polybar"],
    "$HOME/.config/": ["config", "cfg"],
    "$HOME/.shell/": ["shell"],
    "$HOME/forks": ["forks", "frk"],
    "$HOME/projects": ["projects", "prj"],
    "$HOME/projects/algorithmia": ["algorithmia", "alg"],
    "$HOME/projects/algorithmia/cpp": ["algorithmia-cpp", "algcpp"],
    "$HOME/projects/algorithmia/js": ["algorithmia-js", "algjs"],
    "$HOME/projects/goto": ["goto"],
    "$HOME/infrastructure": ["infra", "i"],
    "$HOME/infrastructure/docker": ["infra-docker", "infrad", "id"],
    "$HOME/infrastructure/terraform": ["infra-terraform", "infrat", "it"],
    "$HOME/infrastructure/ansible": ["infra-terraform", "infraa", "ia"],
    "$HOME/projects/musicman": ["musicman", "mm"],
    "$HOME/projects/musicman/server": ["musicman-server", "mm-server", "mms"],
    "$HOME/projects/musicman/cli": ["musicman-cli", "mmcli"],
    "$HOME/projects/musicman/client": ["musicman-client", "mmclient"],
    "$HOME/projects/onemission-deploy": ["onemission-deploy", "omd"],
    "$HOME/projects/onemission-deploy/onemission": ["onemission", "om"],
    "$HOME/projects/onemission-deploy/onemission/public/content/themes/fh_boilerplate-child": [
      "onemission-theme",
      "omt"
    ],
    "$HOME/projects/onemission-deploy/onemission/public/content/plugins": ["onemission-plugins", "omp"],
    "$HOME/projects/better-musician": ["better-musician", "bm"],
    "$HOME/projects/better-musician/client": ["better-musician-client", "bmc"],
    "$HOME/projects/better-musician/server": ["better-musician-server", "bm-server", "bms"],
    "$HOME/projects/debugger-256": ["debugger-256", "debugger256", "dbg"],
    "$HOME/projects/prettyjson-256": ["prettyjson-256", "prettyjson256", "pjson"],
    "$HOME/projects/api-scheduler": ["scheduler", "spadash", "sd"],
    "/var/log": ["log"]
  },

  "runCommands": [
    {
      "name": "OneMission View Logs",
      "command": "tail -n 300 -f",
      "aliases": ["onemission-logs", "om-logs", "omlogs"],
      "directory": "$HOME/projects/onemission-deploy/onemission/public/logs"
    },
    {
      "name": "Musicman Client Dev",
      "command": "npm run dev ",
      "aliases": ["musicman", "mm"],
      "directory": "$HOME/projects/musicman/client"
    },
    {
      "name": "Musicman Client Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-lint", "mm-lint", "mm-l"],
      "directory": "$HOME/projects/musicman/client"
    },
    {
      "name": "Musicman API Dev ",
      "command": "npm run dev ",
      "aliases": ["musicman-api", "mma"],
      "directory": "$HOME/projects/musicman/server"
    },
    {
      "name": "Musicman API Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-api-lint", "mma-lint", "mma-l"],
      "directory": "$HOME/projects/musicman/server"
    },
    {
      "name": "Musicman CLI Dev ",
      "command": "npm run dev ",
      "aliases": ["musicman-cli", "mmc"],
      "directory": "$HOME/projects/musicman/cli"
    },
    {
      "name": "Musicman CLI Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-cli-lint", "mmc-lint", "mmc-l"],
      "directory": "$HOME/projects/musicman/cli"
    },
    {
      "name": "Spadash Scheduler Dev ",
      "command": "npm run dev ",
      "aliases": ["scheduler", "spadash", "sd"],
      "directory": "$HOME/projects/api-scheduler"
    },
    {
      "name": "Spadash Scheduler Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["scheduler-lint", "spadash-lint", "sd-l", "sdl"],
      "directory": "$HOME/projects/api-scheduler"
    },
    {
      "name": "BetterMusician Client Dev",
      "command": "npm run dev ",
      "aliases": ["better-musician-client", "bmc"],
      "directory": "$HOME/projects/better-musician/client"
    },
    {
      "name": "BetterMusician Client Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["better-musician-lint", "bmc-lint", "bmc-l", "bmcl"],
      "directory": "$HOME/projects/better-musician/client"
    },
    {
      "name": "BetterMusician API Dev",
      "command": "npm run dev ",
      "aliases": ["better-musician-server", "bm-server", "bms"],
      "directory": "$HOME/projects/better-musician/server"
    },
    {
      "name": "BetterMusician API Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["better-musician-lint", "bm-server-lint", "bms-l", "bmsl"],
      "directory": "$HOME/projects/better-musician/server"
    }
  ],

  "tmuxCommands": [
    {
      "name": "Docker Containers & Images",
      "command": "tmux new-window -n docker && tmux split-window -v -p 60 && tmux split-window -v -p 50 -t 0 && tmux send-keys -t 0 'loop \\\"docker images | xcols docker_images\\\"' C-M && tmux send-keys -t 1 'loop \\\"docker ps -a | xcols docker_containers\\\"' C-M && tmux select-pane -t 2",
      "aliases": ["docker", "d"]
    }
  ]
}
