{
  "directories": {
    "$HOME/resources": ["resources", "rsc", "dbg"],
    "$HOME/resources/config/dotfiles": ["dotfiles"],
    "$HOME/resources/config/dotfiles/.config": ["resources-config", "rsc-config", "rsc-cfg"],
    "$HOME/resources/config/dotfiles/.shell": ["resources-shell", "rsc-shell"],
    "$HOME/resources/notes/commands": ["resources-commands", "commands", "cmds"],
    "$HOME/resources/notes/languages": ["resources-languages", "languages", "lang"],
    "$HOME/.config/i3": ["i3"],
    "$HOME/.config/polybar": ["polybar"],
    "$HOME/.config/tmux": ["tmux"],
    "$HOME/.config/vim": ["vim"],
    "$HOME/.config/": ["config", "cfg"],
    "/var/log": ["log"],
    "$HOME/.shell/": ["shell"],
    "/home/$USER/projects": ["projects", "prj"],
    "/home/$USER/projects/musicman": ["musicman", "mm"],
    "/home/$USER/projects/musicman/server": ["musicman-server", "mm-server", "mms"],
    "/home/$USER/projects/musicman/cli": ["musicman-cli", "mmcli"],
    "/home/$USER/projects/musicman/client": ["musicman-client", "mmclient"],
    "/home/$USER/projects/better-musician": ["better-musician", "bm"],
    "/home/$USER/projects/better-musician-api": ["better-musician-api", "bm-api", "bma"],
    "/home/$USER/projects/debugger-256": ["debugger-256", "debugger256", "dbg"]
  },

  "runCommands": [
    {
      "name": "Musicman Client Dev",
      "command": "npm run dev ",
      "aliases": ["musicman", "mm"],
      "directory": "/home/$USER/projects/musicman/client"
    },
    {
      "name": "Musicman Client Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-lint", "mm-lint", "mm-l"],
      "directory": "/home/$USER/projects/musicman/client"
    },
    {
      "name": "Musicman API Dev ",
      "command": "npm run dev ",
      "aliases": ["musicman-api", "mma"],
      "directory": "/home/$USER/projects/musicman/server"
    },
    {
      "name": "Musicman API Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-api-lint", "mma-lint", "mma-l"],
      "directory": "/home/$USER/projects/musicman/server"
    },
    {
      "name": "Musicman CLI Dev ",
      "command": "npm run dev ",
      "aliases": ["musicman-cli", "mmc"],
      "directory": "/home/$USER/projects/musicman/cli"
    },
    {
      "name": "Musicman CLI Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["musicman-cli-lint", "mmc-lint", "mmc-l"],
      "directory": "/home/$USER/projects/musicman/cli"
    },
    {
      "name": "BetterMusician Client Dev",
      "command": "npm run dev ",
      "aliases": ["better-musician", "bm"],
      "directory": "/home/$USER/projects/better-musician"
    },
    {
      "name": "BetterMusician Client Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["better-musician-lint", "bm-lint", "bm-l"],
      "directory": "/home/$USER/projects/better-musician"
    },
    {
      "name": "BetterMusician API Dev",
      "command": "npm run dev ",
      "aliases": ["better-musician-api", "bm-api"],
      "directory": "/home/$USER/projects/better-musician-api"
    },
    {
      "name": "BetterMusician API Lint",
      "command": "npm run lint:watch:all",
      "aliases": ["better-musician-api-lint", "bm-api-lint", "bma-l"],
      "directory": "/home/$USER/projects/better-musician-api"
    }
  ],

  "tmuxCommands": []
}
